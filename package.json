{
  "name": "pokecard-nft",
  "version": "0.0.4",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --environment=dev && ng serve --open",
    "compile-contract": "npx hardhat compile",
    "deploy-contract": "npx hardhat --network goerli run scripts/deploy.ts",
    "mint": "npx ts-node scripts/mint-nft.ts",
    "build": "npm run config -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.6",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@openzeppelin/contracts": "^4.7.3",
    "assert": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.1",
    "hardhat": "^2.10.2",
    "https-browserify": "^1.0.0",
    "ngx-spinner": "^13.1.1",
    "os-browserify": "^0.3.0",
    "rxjs": "~7.5.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tslib": "^2.3.0",
    "url": "^0.11.0",
    "web3": "^1.7.3",
    "yargs": "^17.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "ethers": "^5.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  }
}
